# Calculate format=kde
[Appearance]
#тема
ColorScheme=Calculate

[General]
#формат заголовка вкладки (заголовок окна устанавливается оболочкой)
LocalTabTitleFormat=%w
#удалённый сеанс (заголовок окна устанавливается оболочкой)
RemoteTabTitleFormat=%w
#отображать кнопки "создать" и "закрыть" вкладку на панели вкладок
ShowNewAndCloseTabButtons=true
#расположение табов
TabBarPosition=1

#?pkg(kde-base/konsole)==4.2.1#
[Interaction Options]
#исправим баг захвата текста мышкой при двойном клике
WordCharacters='"`(){}
#pkg#
